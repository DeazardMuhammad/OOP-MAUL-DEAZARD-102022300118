<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="LoginController">
    <children>
        <Label text="Username" layoutX="50" layoutY="70"/>
        <TextField fx:id="usernameField" layoutX="150" layoutY="70"/>
        <Label text="Password" layoutX="50" layoutY="120"/>
        <PasswordField fx:id="passwordField" layoutX="150" layoutY="120"/>
        <Button text="Login" layoutX="150" layoutY="170" onAction="#handleLogin"/>
    </children>
</AnchorPane>
